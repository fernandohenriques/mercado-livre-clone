{
  "name": "mercado-livre-clone",
  "version": "1.0.0",
  "description": "The Mercode Livre short app clone",
  "main": "pages/index.js",
  "scripts": {
    "dev": "dotenv -- bash -c 'next -p $PORT'",
    "build": "next build",
    "start": "dotenv -- bash -c 'next start -p $PORT'",
    "test": "jest",
    "lint:js": "eslint . --fix",
    "lint:css": "stylelint **/*.scss --fix",
    "test:coverage": "jest --coverage --colors"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "autoprefixer": "^9.7.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^4.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^9.3.2",
    "next-redux-wrapper": "^4.0.1",
    "node-sass": "^7.0.0",
    "normalize.css": "^8.0.1",
    "object-hash": "^2.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "sass-rem": "^2.0.1",
    "sass-resources-loader": "^2.0.1",
    "svg-react-loader": "^0.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@testing-library/react": "^9.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0",
    "faker": "^4.1.0",
    "husky": "^3.0.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "nock": "^11.7.0",
    "redux-mock-store": "^1.5.3",
    "stylelint": "^11.1.1",
    "stylelint-config-primer": "^8.2.1",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-config-standard": "^19.0.0"
  },
  "repository": "https://github.com/fernandohenriques/mercado-livre-clone.git",
  "author": "Fernando Henriques",
  "license": "MIT"
}
